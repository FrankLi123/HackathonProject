{
    "manifest_version": 3,
    "name": "sAveIt - Paywall Bypasser & Archiver",
    "version": "1.2",
    "description": "A Chrome extension to bypass paywalls and archive web pages",
    "permissions": [
        "activeTab",
        "scripting",
        "storage"
    ],
    "host_permissions": [
        "http://your-backend-service.com/*"
    ],
    "action": {
        "default_popup": "popup.html"
    },
    "background": {
        "service_worker": "background.js"
    }
}